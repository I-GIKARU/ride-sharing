# Kenyan Ride Share Flutter App Configuration - Example
# Copy this file to .env and update the values as needed

# Backend API Configuration
API_BASE_URL=http://localhost:8080
API_VERSION=/api/v1

# App Information
APP_NAME=Kenyan Ride Share
APP_VERSION=1.0.0

# Environment
ENVIRONMENT=development

# Debug Configuration
DEBUG_MODE=true
ENABLE_LOGGING=true

# API Timeouts (in seconds)
API_TIMEOUT=30
CONNECTION_TIMEOUT=10

# Cache Configuration
CACHE_DURATION=300

# Map Configuration (Nairobi, Kenya coordinates)
DEFAULT_LATITUDE=-1.286389
DEFAULT_LONGITUDE=36.817223
DEFAULT_ZOOM=15.0

# Production Example:
# API_BASE_URL=https://api.kenyanrideshare.com
# ENVIRONMENT=production
# DEBUG_MODE=false
# ENABLE_LOGGING=false
